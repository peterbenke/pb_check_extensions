{
    "name": "peterbenke/pb-check-extensions",
    "type": "typo3-cms-extension",
    "description": "Checks, if there are updates available for installed extensions and sends an email",
    "keywords": ["TYPO3", "extension", "backend", "scheduler"],
    "homepage": "https://www.typomotor.de",
    "authors": [
        {
            "name": "Peter Benke",
            "role": "Developer",
            "homepage": "https://www.typomotor.de"
        }
    ],
    "license": [
        "GPL-2.0+"
    ],
    "require": {
        "typo3/cms-core": "^12.4"
    },
    "autoload": {
        "psr-4": {
            "PeterBenke\\PbCheckExtensions\\": "Classes"
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "pb_check_extensions"
        }
    }
}
